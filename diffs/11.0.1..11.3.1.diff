- */
-const withCustomStringsXml: ConfigPlugin = (config) =>
-  withStringsXml(config, (modConfig) => {
-    modConfig.modResults = AndroidConfig.Strings.setStringItem(
-      [
-        {
-          _: "true",
-          $: {
-            name: "expo_splash_screen_status_bar_translucent",
-            translatable: "false",
-          },
-        },
-      ],
-      modConfig.modResults,
-    )
-    return modConfig
-  })
