+    >
+      <Animated.View
+        style={[
+          $checkboxInner,
+          { backgroundColor: onBackgroundColor },
+          $innerStyleOverride,
+          useAnimatedStyle(() => ({ opacity: withTiming(on ? 1 : 0) }), [on]),
+        ]}
+      >
+        <Image
+          source={iconRegistry[checkboxIcon] || iconRegistry.check}
+          style={[$checkboxDetail, { tintColor: iconTintColor }, $detailStyleOverride]}
+        />
+      </Animated.View>
+    </View>
+  )
